{
  "buildCommand": "cd Frontend && npm ci && npm run build",
  "outputDirectory": "Frontend/dist",
  "functions": {
    "api/index.py": {
      "runtime": "python@3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/dist/$1"
    }
  ]
}
