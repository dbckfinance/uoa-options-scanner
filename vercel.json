{
  "buildCommand": "cd Frontend && npm ci && npm run build",
  "outputDirectory": "Frontend/dist",
  "functions": {
    "api/analyze/[ticker].py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/analyze/(.*)",
      "dest": "/api/analyze/[ticker].py"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/dist/$1"
    }
  ]
}
